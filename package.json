{
  "name": "tui-grid",
  "version": "1.5.1",
  "description": "TOAST UI Grid : Powerful data grid control supported by TOAST UI",
  "main": "dist/grid.js",
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "karma start karma.conf.js",
    "test:ne": "karma start karma.conf.ne.js",
    "deploy": "npm run deploy:normal & npm run deploy:combine",
    "deploy:normal": "webpack --production & webpack --production --minify",
    "deploy:combine": "webpack --production --combine & webpack --production --combine --minify",
    "docs": "npm run docs:jsdoc && npm run docs:copy",
    "docs:jsdoc": "./node_modules/.bin/jsdoc -t ./node_modules/fedoc-template -c ./jsdoc.conf.json",
    "docs:copy": "cpy dist/grid.{js,css} doc/dist & cpy dist/images/* doc/dist/images"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nhnent/tui.grid"
  },
  "author": "NHN Ent. FE Development Lab",
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.12.4",
    "copy-webpack-plugin": "^3.0.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.25.0",
    "es5-shim": "^4.5.9",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "express": "^4.12.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fedoc-template": "git+https://14bfd38a036366f7ca90355d202038126127bd01:x-oauth-basic@github.nhnent.com/fe/share-fedoc-template.git#develop",
    "file-loader": "^0.9.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-ajax": "^2.99.0",
    "jasmine-core": "^2.3.4",
    "jasmine-jquery": "^2.0.5",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "karma": "^1.1.1",
    "karma-browserstack-launcher": "^0.1.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.1",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webdriver-launcher": "git://github.com/everedifice/karma-webdriver-launcher.git",
    "karma-webpack": "^1.8.0",
    "phantomjs": "^1.9.18",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "backbone": "^1.1.2"
  }
}
