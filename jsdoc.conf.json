{
    "source": {
        "include": [
            "src/js",
            "README.md"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "applicationName": "tui-grid",
        "name": "Grid",
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "recurse": true,
        "template": "node_modules/tui-jsdoc-template",
        "name": "Toast UI Grid",
        "destination": "doc",
        "tutorials": "examples"
    }
}
